{
  "name": "@asany/sunmao",
  "version": "1.0.30",
  "scripts": {
    "start": "dumi dev",
    "docs:build": "dumi build",
    "docs:deploy": "gh-pages -d docs-dist",
    "build": "father build",
    "deploy": "npm run docs:build && npm run docs:deploy",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable && npm run build",
    "postpublish": "pinst --enable",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
    "postversion": "git push --follow-tags"
  },
  "files": [
    "dist"
  ],
  "module": "dist/esm/index.js",
  "typings": "dist/esm/index.d.ts",
  "repository": "https://github.com/limaofeng/asany-sunmao",
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "size-limit": [
    {
      "path": "dist/editor.cjs.production.min.js",
      "limit": "2 MB",
      "ignore": [
        "fonteditor-core"
      ]
    },
    {
      "path": "dist/editor.esm.js",
      "limit": "1 MB",
      "ignore": [
        "fonteditor-core"
      ]
    }
  ],
  "dependencies": {
    "@asany/sortable": "^0.1.36",
    "@daybrush/drag": "^0.19.3",
    "@react-hook/merged-ref": "^1.3.2",
    "@scena/react-guides": "^0.17.1",
    "lodash": "^4.17.21",
    "moveable-helper": "^0.4.0",
    "overlayscrollbars": "^1.13.2",
    "overlayscrollbars-react": "^0.3.0",
    "react": "^18.0.0",
    "react-flow-renderer": "^10.3.12",
    "react-is": "^18.2.0",
    "react-moveable": "^0.33.0",
    "react-selecto": "^1.17.0",
    "react-transition-group": "^4.4.2",
    "react-use": "^17.4.0",
    "reflect-metadata": "^0.1.13",
    "scenejs": "^1.6.0",
    "styled-components": "^5.3.5",
    "antd": "^4.21.7"
  },
  "devDependencies": {
    "@apollo/client": "^3.6.9",
    "@asany/icons": "^0.2.13",
    "@asany/tree": "^0.1.28",
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@tailwindcss/postcss7-compat": "^2.2.17",
    "@testing-library/jest-dom": "^5.15.1",
    "@testing-library/react": "^13.0.0",
    "@types/jest": "^27.0.3",
    "@types/lodash": "^4.14.182",
    "@types/react-is": "^17.0.3",
    "@types/react-transition-group": "^4.4.5",
    "@types/styled-components": "^5.1.25",
    "@umijs/fabric": "^2.8.1",
    "@umijs/test": "^3.0.5",
    "autoprefixer": "9",
    "babel-plugin-import": "^1.13.5",
    "conventional-changelog-cli": "^2.2.2",
    "dumi": "^1.1.0",
    "father": "^4.0.0-rc.2",
    "file-loader": "^6.2.0",
    "gh-pages": "^3.0.0",
    "graphql": "^16.5.0",
    "husky": "^8.0.1",
    "lint-staged": "^10.0.7",
    "pinst": "^3.0.0",
    "prettier": "^2.2.1",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18.2.0",
    "size-limit": "^7.0.8",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat",
    "yorkie": "^2.0.0"
  }
}
